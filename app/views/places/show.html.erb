<h1><%= @place["name"]%></h1>

<p>
    <% for post in @posts %>
        <h2> <%= post["title"]%> </h2>
        <h6> <%= post["posted_on"]%> </h6>
        <p> <%= post["description"]%> </p>
    <% end %>
</p>

<p>
    <a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a>
</p>
<p>
    <a href="/places">Back to all Places</a>
</p>